\end_inset


\begin_inset Formula $4(a+b)^{2}-9(a-b)^{2}=[2(a+b)]^{2}-[3(a-b)]^{2}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{(M)M}=[2(a+b)+3(a-b)][2(a+b)-3(a-b)]$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{(M)M}=(2a+2b+3a-3b)(2a+2b-3a+3b)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{(M)M}=(5a-b)(-a+5a)=(5a-b)(5b-a)$
\end_inset

.
\end_layout

