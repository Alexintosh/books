\end_layout

\begin_layout 解*
\begin_inset Formula $2ab-a^{2}-b^{2}+1=1-\big(a^{2}-2ab+b^{2}\big)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{M}=1-(a-b)^{2}=[1+(a-b)][1-(a-b)]$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{M}=(1+a-b)(1-a+b)$
\end_inset

.
\end_layout

