\end_inset

;
\end_layout

\begin_layout 审题*
(2)
\begin_inset space \space{}
\end_inset


\begin_inset Formula $4(a+b)^{2}=[2(a+b)]^{2}$
\end_inset

,
\begin_inset Newline newline
