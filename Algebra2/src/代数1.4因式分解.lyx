\begin_inset Formula $a$
\end_inset

,把
\begin_inset Formula $y^{6}$
\end_inset

看作是
\begin_inset Formula $b$
\end_inset

,原式就能用公式
\end_layout

\begin_layout CenterLine
\begin_inset Formula $a^{3}-3a^{2}b+3ab^{2}-b^{3}=(a-b)^{3}$
\end_inset


\end_layout

\begin_layout Standard
来分解因式.
\end_layout

\begin_layout 解*
\begin_inset Formula $x^{18}-3x^{12}y^{6}+3x^{6}y^{12}-y^{18}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{M}=\big(x^{6}\big)^{3}-3\big(x^{6}\big)^{2}\big(y^{6}\big)+3\big(x^{6}\big)\big(y^{6}\big)^{2}-\big(y^{6}\big)^{3}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{M}=\big(x^{6}-y^{6}\big)^{3}=\big[\big(x^{3}+y^{3}\big)\big(x^{3}-y^{3}\big)\big]^{3}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{M}=\big[(x+y)\big(x^{2}-xy+y^{2}\big)(x-y)\big(x^{2}+xy+y^{2}\big)\big]^{3}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{M}=(x+y)^{3}\big(x^{2}-xy+y^{2}\big)^{3}(x-y)^{3}\big(x^{2}+xy+y^{2}\big){}^{3}$
\end_inset

.
\end_layout

\begin_layout Example
分解因式:
\begin_inset Formula $a^{2}-2ab+b^{2}-5a+5b+6$
\end_inset

.
\end_layout

\begin_layout 解*
\begin_inset Formula $a^{2}-2ab+b^{2}-5a+5b+6$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{M}=(a-b)^{2}-5(a-b)+6$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{M}=[(a-b)-3][(a-b)-2]$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{M}=(a-b-c)(a-b-2)$
\end_inset

.
\end_layout

\begin_layout Example
分解因式:
\begin_inset Newline newline
\end_inset

(1) 
\begin_inset Formula $-a^{2}+6ab-9b^{2}$
\end_inset

;
\begin_inset space \qquad{}
\end_inset

(2) 
\begin_inset Formula $-x^{2}-3x+4$
\end_inset

.
\end_layout

\begin_layout 解*
(1)
